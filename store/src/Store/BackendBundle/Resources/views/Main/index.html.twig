        <!-- 5. $UPLOADS_CHART =============================================================================

                        Uploads chart
        -->
        <!-- Javascript -->
        <script>
            init.push(function () {
                var uploads_data = [
                    { month: '2014-01-31', v: 40 },
                    { month: '2014-02-28', v: 10 },
                    { month: '2014-03-31', v: 15 },
                    { month: '2014-04-30', v: 12 },
                    { month: '2014-05-31', v: 5  },
                    { month: '2014-06-30', v: 15  },
                    { month: '2014-07-31', v: 18 },
                    { month: '2014-08-30', v: 14 },
                    { month: '2014-09-31', v: 10 },
                    { month: '2014-10-30', v: 20 },
                    { month: '2014-11-31', v: 15 },
                    { month: '2014-12-30', v: 17 },
                ];
                Morris.Line({
                    element: 'hero-graph',
                    data: uploads_data,
                    xkey: 'month',
                    ykeys: ['v'],
                    labels: ['Value'],
                    lineColors: ['#fff'],
                    lineWidth: 2,
                    pointSize: 4,
                    gridLineColor: 'rgba(255,255,255,.5)',
                    resize: true,
                    gridTextColor: '#fff',
                    xLabels: "day",
                    xLabelFormat: function(d) {
                        return ['Jan','Fev','Mar','Avr','Mai','Juin','Juil','Aout','Sep','Oct','Nov', 'Dec'][d.getMonth()] + ' ' + d.getDate();
                    }
                });
            });
        </script>
        <!-- / Javascript -->

